{
  "project": {
    "name": "{{PROJECT_NAME}}",
    "root_path": "{{PROJECT_ROOT_PATH}}",
    "user": "{{USER_NAME}}",
    "type": "{{PROJECT_TYPE}}"
  },
  "services": {
    "backend": {
      "port": "{{BACKEND_PORT}}",
      "health_endpoint": "{{BACKEND_HEALTH_ENDPOINT}}",
      "start_command": "{{BACKEND_START_COMMAND}}"
    },
    "frontend": {
      "port": "{{FRONTEND_PORT}}",
      "health_endpoint": "{{FRONTEND_HEALTH_ENDPOINT}}",
      "start_command": "{{FRONTEND_START_COMMAND}}"
    },
    "database": {
      "type": "{{DATABASE_TYPE}}",
      "version": "{{DATABASE_VERSION}}",
      "port": "{{DATABASE_PORT}}",
      "host": "{{DATABASE_HOST}}",
      "user": "{{DATABASE_USER}}",
      "extensions": ["{{DATABASE_EXTENSIONS}}"]
    }
  },
  "monitoring": {
    "script_path": "{{MONITORING_SCRIPT_PATH}}",
    "thresholds": {
      "ok": 60,
      "attention": 80,
      "danger": 90
    }
  },
  "structure": {
    "docs_path": "{{DOCS_PATH}}",
    "transitions_path": "{{TRANSITIONS_PATH}}",
    "archives_path": "{{ARCHIVES_PATH}}",
    "protocols_path": "{{PROTOCOLS_PATH}}",
    "backend_path": "{{BACKEND_PATH}}",
    "frontend_path": "{{FRONTEND_PATH}}"
  },
  "critical_files": {
    "pgvector_repo": "{{PGVECTOR_PATH}}",
    "start_script": "{{START_SCRIPT_PATH}}",
    "claude_instructions": "{{CLAUDE_INSTRUCTIONS_FILE}}",
    "machine_instructions": "{{MACHINE_INSTRUCTIONS_FILE}}"
  },
  "git": {
    "main_branch": "{{MAIN_BRANCH}}",
    "remote": "{{REMOTE_NAME}}"
  }
}