{
  "project": {
    "name": "django_project",
    "root_path": "/path/to/your/django_project/",
    "user": "username",
    "type": "web_app"
  },
  "services": {
    "backend": {
      "port": 8000,
      "health_endpoint": "/health/",
      "start_command": "cd backend && python manage.py runserver 8000"
    },
    "frontend": {
      "port": 3000,
      "health_endpoint": "/",
      "start_command": "cd frontend && npm start"
    },
    "database": {
      "type": "postgresql",
      "version": "14.x",
      "port": 5432,
      "host": "localhost",
      "user": "username",
      "extensions": []
    }
  },
  "monitoring": {
    "script_path": "~/scripts/claude_monitor.sh",
    "thresholds": {
      "ok": 60,
      "attention": 80,
      "danger": 90
    }
  },
  "structure": {
    "docs_path": "docs",
    "transitions_path": "docs/transitions",
    "archives_path": "docs/transitions/archives",
    "protocols_path": "docs/protocols",
    "backend_path": "backend",
    "frontend_path": "frontend"
  },
  "critical_files": {
    "start_script": "backend/manage.py",
    "claude_instructions": "CLAUDE.md",
    "machine_instructions": "docs/protocols/INSTRUCTIONS_MACHINE_AUTOMATIQUES.md"
  },
  "git": {
    "main_branch": "main",
    "remote": "origin"
  }
}