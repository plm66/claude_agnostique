{
  "project": {
    "name": "node_project",
    "root_path": "/path/to/your/node_project/",
    "user": "username",
    "type": "backend_api"
  },
  "services": {
    "backend": {
      "port": 3000,
      "health_endpoint": "/health",
      "start_command": "npm run dev"
    },
    "frontend": {
      "port": null,
      "health_endpoint": null,
      "start_command": null
    },
    "database": {
      "type": "mongodb",
      "version": "6.x",
      "port": 27017,
      "host": "localhost",
      "user": "username",
      "extensions": []
    }
  },
  "monitoring": {
    "script_path": "~/scripts/claude_monitor.sh",
    "thresholds": {
      "ok": 60,
      "attention": 80,
      "danger": 90
    }
  },
  "structure": {
    "docs_path": "docs",
    "transitions_path": "docs/transitions",
    "archives_path": "docs/transitions/archives",
    "protocols_path": "docs/protocols",
    "backend_path": "src",
    "frontend_path": null
  },
  "critical_files": {
    "start_script": "package.json",
    "claude_instructions": "CLAUDE.md",
    "machine_instructions": "docs/protocols/INSTRUCTIONS_MACHINE_AUTOMATIQUES.md"
  },
  "git": {
    "main_branch": "main",
    "remote": "origin"
  }
}